.btn{
  position: absolute;
  width: 50px;
  height:50px;
  border-radius: 50%;
  background-color: blue;
  z-index: 999;
  text-align: center;
  line-height: 30px;
  color:#fff;
}
.canvas{
  width: 370px;
  height:300px;
  border:1px solid #000;
  position: relative;
}
.dot{
  width: 10px;
  height:10px;
  background: #000;
  display: inline-block;
  border-radius: 50%;
  position: absolute;
}